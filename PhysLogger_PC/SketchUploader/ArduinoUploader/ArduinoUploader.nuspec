<?xml version="1.0"?>
<package >
  <metadata>
    <id>ArduinoUploader</id>
    <version>$version$</version>
    <title>ArduinoUploader</title>
    <authors>Christophe Diericx</authors>
    <owners>Christophe Diericx</owners>
    <projectUrl>https://github.com/christophediericx/ArduinoSketchUploader</projectUrl>
    <iconUrl>https://avatars3.githubusercontent.com/u/19640854?s=140</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A library to upload an Intel HEX file (e.g. compiled sketches) directly to an Arduino over a (USB) serial port without having to use the Arduino IDE (or avrdude).</description>
    <releaseNotes></releaseNotes>
    <copyright></copyright>
    <tags>arduino upload serial avr flash hex</tags>
    <dependencies>
      <dependency id="IntelHexFormatReader" version="2.2.1" />
      <dependency id="SerialPortStream" version="2.1.2" />
    </dependencies>  
  </metadata>
  <files>
	<file src="bin\$configuration$\ArduinoUploader.dll" target="lib\net40\" />
  </files>
</package>
